<h3 class="ui header">{{t "forms.i-i-s-proba10-планир-собес-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proba10-планир-собес-e.датаПланСобес-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.датаПланСобес
      class=(if (v-get validationModel "датаПланСобес" "isInvalid") "error")
      data-test-i-i-s-proba10-планир-собес-e-датаПланСобес=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датаПланСобес" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.иметьПриСебе
      class=(if (v-get validationModel "иметьПриСебе" "isInvalid") "error")
      label=(t "forms.i-i-s-proba10-планир-собес-e.иметьПриСебе-caption")
      data-test-i-i-s-proba10-планир-собес-e-иметьПриСебе=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "иметьПриСебе" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proba10-планир-собес-e.тЧПланирСобес-caption"}}</label>
    {{flexberry-groupedit
      componentName="тЧПланирСобесGroupEdit"
      mainModelProjection=modelProjection
      modelProjection=modelProjection.attributes.тЧПланирСобес
      content=model.тЧПланирСобес
      readonly=readonly
      orderable=false
      class=(if (v-get validationModel "тЧПланирСобес" "isInvalid") "error")
      data-test-i-i-s-proba10-планир-собес-e-тЧПланирСобес=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "тЧПланирСобес" "message")}}
  </div>
</form>
